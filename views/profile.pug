extends default

block content
  div.container
    each argo in argocd
      div.argo
        div.tool-image(style=`background: url('/images/${argo.imgSrc}') top center
        no-repeat; background-size: cover;`)
        a(href=`//${argo.link}`).button
          | #{argo.toolname} #{argo.lastname} #{argo.bio}
